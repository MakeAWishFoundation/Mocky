use_frameworks!

target 'Mocky_Example_iOS' do
  platform :ios, '8.3'
  target 'Mocky_Tests_iOS' do
    inherit! :search_paths
  	pod 'SwiftyMocky', :path => './'
  end
end

target 'Mocky_Example_tvOS' do
  platform :tvos, '9.0'
  target 'Mocky_Tests_tvOS' do
    inherit! :search_paths
    pod 'SwiftyMocky', :path => './'
  end
end
